{
  "name": "media-lifestyle-apps",
  "version": "0.0.0",
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "build": "ng build",
    "watch": "ng build --watch --configuration development",
    "test": "jest --config=jest.config.all.ts",
    "test-shared": "jest --config=jest.config.shared.ts",
    "test-recipes": "jest --config=jest.config.recipes.ts",
    "test-media": "jest --config=jest.config.series-movies.ts",
    "start-recipes": "ng serve recipes --port 4200",
    "publish-recipes": "ngrok http 4200 --host-header=\"localhost:4200\"",
    "build-recipes": "npm run build recipes --prod",
    "update-version-recipes": "@powershell -NoProfile -ExecutionPolicy Unrestricted -Command .\\projects\\recipes\\src\\update-version-patch.ps1",
    "deploy-to-firebase-recipes": "firebase deploy --only hosting:recipes-purchases",
    "commit-new-version-recipes": "git add projects/recipes/src/version.json && git commit -m \"[REZEPTE] Neue Version\" && git pull && git push",
    "deploy-recipes": "npm run update-version-recipes && npm run build-recipes && npm run commit-new-version-recipes && npm run deploy-to-firebase-recipes",
    "start-media": "ng serve series-movies --port 4201",
    "publish-media": "ngrok http 4201 --host-header=\"localhost:4201\"",
    "build-media": "npm run build series-movies --prod",
    "update-version-media": "@powershell -NoProfile -ExecutionPolicy Unrestricted -Command .\\projects\\series-movies\\src\\update-version-patch.ps1",
    "deploy-to-firebase-media": "firebase deploy --only hosting:media-series-movies-shows",
    "commit-new-version-media": "git add projects/series-movies/src/version.json && git commit -m \"[MEDIA] Neue Version\" && git pull && git push",
    "deploy-media": "npm run update-version-media && npm run build-media && npm run commit-new-version-media && npm run deploy-to-firebase-media",
    "deploy-functions": "firebase deploy --only functions"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "^17.1.3",
    "@angular/cdk": "^17.1.2",
    "@angular/common": "^17.1.3",
    "@angular/compiler": "^17.1.3",
    "@angular/core": "^17.1.3",
    "@angular/fire": "^17.0.1",
    "@angular/forms": "^17.1.3",
    "@angular/material": "^17.1.2",
    "@angular/material-moment-adapter": "^17.1.2",
    "@angular/platform-browser": "^17.1.3",
    "@angular/platform-browser-dynamic": "^17.1.3",
    "@angular/router": "^17.1.3",
    "@angular/service-worker": "^17.1.3",
    "@ngx-translate/core": "^15.0.0",
    "@ngx-translate/http-loader": "^8.0.0",
    "cheerio": "^1.0.0-rc.12",
    "date-fns": "^2.29.3",
    "firebase-functions": "^4.8.0",
    "hammerjs": "^2.0.8",
    "jszip": "^3.10.1",
    "lodash.isequal": "^4.5.0",
    "ngx-material-timepicker": "^13.0.3",
    "rxjs": "^7.8.0",
    "ts-node": "^10.9.2",
    "tslib": "^2.3.0",
    "zone.js": "~0.14.3"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "^17.1.3",
    "@angular/cli": "^17.1.3",
    "@angular/compiler-cli": "^17.1.3",
    "@types/hammerjs": "^2.0.41",
    "@types/jest": "^29.5.12",
    "@types/lodash.isequal": "^4.5.6",
    "@types/node": "^12.11.1",
    "jest": "^29.7.0",
    "jest-preset-angular": "^14.0.3",
    "typescript": "~5.3.3"
  }
}
